public class Main {

    public static void main(String[] args) {
        System.out.printf("byte minimum is: %s, and maximum is: %s%n", Byte.MIN_VALUE, Byte.MAX_VALUE);
        System.out.printf("short minimum is: %s, and maximum is: %s%n", Short.MIN_VALUE, Short.MAX_VALUE);
        System.out.printf("int minimum is: %s, and maximum is: %s%n", Integer.MIN_VALUE, Integer.MAX_VALUE);
        System.out.printf("long minimum is: %s, and maximum is: %s%n", Long.MIN_VALUE, Long.MAX_VALUE);
        System.out.printf("float minimum is: %s, and maximum is: %s%n", Float.MIN_VALUE, Float.MAX_VALUE);
        System.out.printf("double minimum is: %s, and maximum is: %s%n", Double.MIN_VALUE, Double.MAX_VALUE);
    }
}
